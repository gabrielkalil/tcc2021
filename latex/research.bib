@book{green,
  title = {{Evangeliza\c{c}\~ao na igreja primitiva}},
  author = {Green, Michael},
  year = {2020},
  month = jan,
  publisher = {{Vida Nova}},
  abstract = {Cinquenta anos ap\'os sua publica\c{c}\~ao, Evangeliza\c{c}\~ao na igreja primitiva tornou-se um cl\'assico ao proporcionar um olhar abrangente sobre os m\'etodos usados pelos primeiros crist\~aos \textemdash{} desde o per\'iodo do Novo Testamento at\'e o terceiro s\'eculo \textemdash{} para divulgar as boas not\'icias ao redor do mundo.Ao descrever a vida da igreja primitiva, Michael Green explora aspectos fundamentais (m\'etodos, motiva\c{c}\~oes e estrat\'egias) da tarefa evangel\'istica que ainda permanecem aplic\'aveis ao trabalho de propagar as boas-novas. Ao longo dos dez cap\'itulos desta segunda edi\c{c}\~ao, o autor avalia os pontos fortes e fracos das abordagens evangel\'isticas dos primeiros crist\~aos e trata dos obst\'aculos ao evangelismo nesse contexto, usando as miss\~oes aos gentios e aos judeus como exemplos de desafios diferentes \`a proclama\c{c}\~ao do evangelho.},
  language = {Portugu\^es}
}
